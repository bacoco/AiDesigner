{
  "mcpServers": {
    "quick-designer-v4": {
      "command": "node",
      "args": ["dist/mcp/src/mcp-server/quick-designer-server.js"],
      "disabled": false,
      "env": {
        "QUICK_DESIGNER_MODE": "ai-powered"
      }
    },
    "shadcn": {
      "command": "npx",
      "args": ["-y", "@jpisnice/shadcn-ui-mcp-server"]
    },
    "tweakcn": {
      "command": "npx",
      "args": ["-y", "tweakcn-mcp-server"]
    },
    "vibe-check": {
      "command": "npx",
      "args": ["-y", "vibe-check-mcp-server"],
      "env": {
        "VIBE_CHECK_API_KEY": "${VIBE_CHECK_API_KEY}",
        "VIBE_CHECK_MODEL": "claude-3-5-sonnet-20241022"
      }
    }
  }
}
