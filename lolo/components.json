{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "app/globals.css",
    "baseColor": "zinc",
    "cssVariables": true
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils"
  },
  "registries": {
    "@originui": "https://originui.com/r/{name}.json",
    "@aceternity": "https://aceternity.com/r/{name}.json",
    "@magicui": "https://magicui.design/r/{name}.json",
    "@kiboui": "https://www.kibo-ui.com/r/{name}.json"
  }
}
